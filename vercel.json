{
  "installCommand": "npm install && cd api && npm install",
  "buildCommand": "echo 'Build completed'",
  "outputDirectory": ".",
  "framework": null,
  "functions": {
    "api/**/*.js": {
      "memory": 512,
      "maxDuration": 10,
      "includeFiles": "**"
    }
  },
  "headers": [
    {
      "source": "/(.*).onnx",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
